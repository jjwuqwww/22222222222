  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Various large input image\n",
    "\n",
    "Fixed variables:\n",
    "* $H$ size: $(\\frac{mn}{9} \\times mn)$\n",
    "* $k \\times k$ filter cropped from $H$\n",
    "\n",
    "Variables:\n",
    "* Input image size: $(O \\times P), (O > m, P > n)$"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "# k filter\n",
    "k = 17\n",
    "H_filter = mat_H_sample[idx_row-k//2:idx_row+k//2+1, idx_col-k//2:idx_col+k//2+1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "# various input image size\n",
    "ar_size = [123, 300, 600, 900, 1200]\n",
    "img_og = Image.open(\"./test imgs/iso chart 4.png\").convert(\"RGB\")\n",
    "\n",
    "mse_hist = []\n",
    "psnr_hist = []\n",
    "\n",
    "for i in range(len(ar_size)):\n",
    "    img_resize = im_resize(img_og, bool_resize=True, size=ar_size[i], resample=Image.BICUBIC)\n",
    "    img_np = np.asarray(img_resize, dtype=np.float32)\n",
    "    o, p = img_np.shape[0], img_np.shape[1]\n",
    "    \n",
    "    # solve circulant\n",
    "    img_circulant = np.asarray(solveCirc2D(img_resize), dtype=np.float32)\n",
    "    \n",
    "    # filter convolve\n",
    "    img_kFilt = np.zeros((o//3, p//3, 3))\n",
    "    tmp_red = convolve(img_np[:,:,0], H_filter)\n",
    "    tmp_grn = convolve(img_np[:,:,1], H_filter)\n",
    "    tmp_blu = convolve(img_np[:,:,2], H_filter)\n",
    "\n",
    "    img_kFilt[:,:,0] = tmp_red[1:o//3*3:3, 0:p//3*3:3]\n",
    "    img_kFilt[:,:,1] = tmp_grn[1:o//3*3:3, 1:p//3*3:3]\n",
    "    img_kFilt[:,:,2] = tmp_blu[1:o//3*3:3, 2:p//3*3:3]\n",
    "\n",
    "    img_kFilt = np.uint8(np.clip(img_kFilt, 0, 255))\n",
    "    img_kFilt = np.float32(img_kFilt)\n",
    "    \n",
    "    mse = ((img_kFilt - img_circulant)**2).mean(axis=None)\n",
    "    psnr = 10*np.log10(255**2/mse)\n",
    "    \n",
    "    mse_hist.append(mse)\n",
    "    psnr_hist.append(psnr)"
   ]
  },
  {
